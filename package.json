{
  "name": "@kmudrick/io-ts-openapi",
  "version": "0.0.6",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/kmudrick/io-ts-openapi"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "commonjs",
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "bin": {
